[
  {
    "kernelSource": {
      "name": "Pissarro-0",
      "repo": "https://github.com/sonrixmomo/Xiaomi_Kernel_OpenSource",
      "branch": "liquid-11.0",
      "device": "pissarro",
      "defconfig": "pissarro_user_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
  {
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9/+archive/refs/heads/android11-release.tar.gz",
    "name": "gcc",
    "binPath": ["bin"]
  },
  {
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/heads/android11-release.tar.gz",
    "name": "gcc64",
    "binPath": ["bin"]
  },
  {
    "repo": "https://github.com/kdrag0n/proton-clang",
    "branch": "master",
    "name": "clang",
    "binPath": ["bin"]
  }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc64/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "gcc/bin/arm-linux-androideabi-",
        "LD": "clang/bin/ld.lld",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LDGOLD": "clang/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "clang/bin/llvm-ar",
        "LLVM_DIS": "clang/bin/llvm-dis",
        "CONFIG_SECTION_MISMATCH_WARN_ONLY": "y"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/sonrixmomo/AnyKernel3",
      "branch": "master"
    }
  }
]